@model IEnumerable<ManagerCovid19.Models.HealthRegistration>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RegisterDateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HowRUFeeling)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Symptoms)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Member)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RegisterDateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HowRUFeeling)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Symptoms)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Member.MemberRegistrationNumber)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.HealthRegistrationID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.HealthRegistrationID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.HealthRegistrationID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
